rtds.hostname=159.43.84.38
rtds.port=14002
rtds.user=KDAC.UAT
rtds.appId=256


@Service
public class MarketDataTestService {

    private final RtdsWebSocketAdapter adapter;

    public MarketDataTestService(RtdsWebSocketAdapter adapter) {
        this.adapter = adapter;
    }

    @PostConstruct
    public void testSubscription() {
        System.out.println("Sending subscription for RIC: TRI.N");
        adapter.subscribe("TRI.N", 2);
    }
}
